<h2>ユーザー新規登録</h2>

<%= form_with model: @user_new, url: users_path, local: true, method: :post do |f| %>
    <%= f.label :name, "name" %>
    <%= f.text_field :name %>

    <%= f.label :email, "email" %>
    <%= f.email_field :email %>

    <%= f.label :password, "password" %>
    <%= f.password_field :password %>

    <%= f.submit "新規登録" %>
<% end %>